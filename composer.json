{
    "name": "takuya/php-pdo-where-helper",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "takuya",
            "email": "takuya.1st+nospam@gmail.com"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "autoload": {
        "psr-4": {
            "PDOHelpers\\": "src/PDOHelpers"
        }
        
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
